# Production Environment Variables

# API Configuration
REACT_APP_API_BASE_URL=https://wellness-app.onrender.com
REACT_APP_ENVIRONMENT=production

# Clerk Authentication
REACT_APP_CLERK_PUBLISHABLE_KEY=pk_live_production_key_here

# Google Gemini API
REACT_APP_GEMINI_API_KEY=production_gemini_api_key_here

# App Configuration
REACT_APP_APP_NAME=WellnessApp
REACT_APP_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_OFFLINE_MODE=true
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_DEBUG=false

# Health Integration
REACT_APP_HEALTH_KIT_ENABLED=true
REACT_APP_GOOGLE_FIT_ENABLED=true